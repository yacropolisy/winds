<div id="tf-why-me">
    <div class="overlay">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <h3>Informations</h3>
                    <br>
                    <ul class="list-inline why-me">
                      <div class="col-sm-8 blog-main">
                        <% @posts.each do |post|%>

                        <div class="blog-post">
                          <h2 class="blog-post-title" style=<%= post.color %>><%= post.title %></h2>
                          <p class="blog-post-meta"><%= post.date%> by <%= post.author%></p>

                          <p><%= simple_format(post.article)%></p>

                        </div><!-- /.blog-post -->
                        <% end %>
                      </div><!-- /.blog-main -->
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>




